{
	"name":"venne/doctrine-module",
	"description":"Doctrine module with macros and components",
	"keywords":["cms", "nette", "venne", "module"],
	"version":"2.0.0",
	"authors":[
		{
			"name":"Josef Kříž",
			"homepage":"http://josef-kriz.cz"
		}
	],
	"require":{
		"php":">=5.3.2",
		"venne/forms-module":"2.0.x-dev",
		"doctrine/orm":"2.3.x",
		"symfony/yaml":"2.1.x"
	},
	"autoload":{
		"psr-0":{
			"DoctrineModule":""
		}
	},
	"extra":{
		"branch-alias":{
			"dev-master":"2.0.x-dev"
		},
		"venne":{
			"configuration":{
				"parameters":{
					"database":{
						"driver":"pdo_mysql",
						"host":"",
						"user":"",
						"password":"",
						"dbname":"",
						"charset":"utf8",
						"proxyDir":"%appDir%/proxy",
						"proxyNamespace":"proxy",
						"port":"",
						"path":"",
						"memory":""
					}
				},
				"extensions":{
					"doctrine":"DoctrineModule\\DI\\DoctrineExtension"
				}
			}
		}
	}
}
